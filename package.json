{
  "name": "agentdev-monorepo",
  "version": "1.0.0",
  "description": "AgentDev with Web UI - Monorepo",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "dev:frontend": "cd apps/frontend && pnpm run dev",
    "build:frontend": "cd apps/frontend && pnpm run build",
    "dev:backend": "cd apps/backend && cargo run --bin agentdev-ui",
    "build": "pnpm run build:frontend && cd apps/backend && cargo build --release",
    "ui": "cd apps/backend && cargo run --bin agentdev-ui"
  }
}
